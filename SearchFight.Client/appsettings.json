{
  "Serilog": {
    "MinimalLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Information"
      }
    }
  },
  "BingSearchSettings": {
    "EngineName": "MSN Search",
    "BaseUrl": "https://api.bing.microsoft.com/v7.0/search",
